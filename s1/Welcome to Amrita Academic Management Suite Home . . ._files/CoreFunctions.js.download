/** Creation of custom JSON object

**/

function JsonObject(){
	this.jsonObj = {};
	this.put = function ( key, value ){
		this.jsonObj[key] = value;
	};
	this.get = function ( key ){
			return this.jsonObj[key];
	};	
	this.toString = function (){	
		return JSON.stringify(this.jsonObj);
	};

};




