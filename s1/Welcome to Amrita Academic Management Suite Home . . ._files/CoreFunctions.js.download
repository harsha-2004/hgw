                		}else{
                			row = obj.name.substring(obj.name.lastIndexOf("_") + 1 );
                		}
                		
                		       		
                		
                		
                		column = control.column;
                		
                		var nextObj;
                		
                		if( code == 37 ){// LEFT ARROW
